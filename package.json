{
  "private": true,
  "name": "minimal-blog",
  "description": "Typography driven, feature-rich blogging theme with minimal aesthetics. Includes tags/categories support and extensive features for code blocks such as live preview, line numbers, and line highlighting.",
  "version": "1.3.9",
  "author": "LekoArts <hello@lekoarts.de>",
  "license": "0BSD",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build && gh-pages -d public -b master --no-history",
    "develop": "gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "ghpageshelp": "gh-pages --help"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-minimal-blog": "^2.7.3",
    "gatsby": "^2.25.3",
    "gatsby-plugin-google-analytics": "^2.4.1",
    "gatsby-plugin-manifest": "^2.5.2",
    "gatsby-plugin-netlify": "^2.4.0",
    "gatsby-plugin-offline": "^3.3.3",
    "gatsby-plugin-sitemap": "^2.5.1",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.13",
    "gatsby-remark-copy-linked-files": "^2.4.0",
    "gatsby-remark-graph": "^0.2.3",
    "gatsby-remark-images-grid": "^1.1.2",
    "gatsby-remark-katex": "^3.4.0",
    "gatsby-transformer-remark": "^2.11.0",
    "prismjs": "^1.22.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rehype-katex": "^4.0.0",
    "remark-math": "^4.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "gh-pages": "^3.1.0"
  }
}
